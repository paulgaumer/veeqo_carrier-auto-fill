{
  "name": "Veeqo Carrier Auto-fill ",
  "description": "Quickly add a carrier for development purposes",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/veeqoIcon-16.png",
      "32": "/icons/veeqoIcon-32.png",
      "48": "/icons/veeqoIcon-48.png"
    }
  },
  "icons": {
    "16": "/icons/veeqoIcon-16.png",
    "32": "/icons/veeqoIcon-32.png",
    "48": "/icons/veeqoIcon-48.png"
  }
}
